<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <remove name="aspNetCore" />
      <add name="aspNetCore"
           path="*"
           verb="*"
           modules="AspNetCoreModule"
           resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="dotnet"
                arguments="run"
                stdoutLogEnabled="true"
                shutdownTimeLimit="2"
                stdoutLogFile=".\" />
  </system.webServer>
</configuration>